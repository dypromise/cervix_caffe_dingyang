name : "FCN"
############################################## data ################################################
layer {
  name: "data"
  type: "ImageData"
  top: "data"
  top: "label"
  transform_param {
    mirror: false
    #crop_size: 299
    #mean_value: 97.5
    #mean_value: 91.5
    #mean_value: 121.2
  }
  image_data_param {
    root_folder: "/mnt/lustre/dingyang/test_399x4/" 
    source: "/mnt/lustre/dingyang/fcn_test_list.txt"
    batch_size: 1
    #new_height: 299
    #new_width: 299
    #label_type: PIXEL
    shuffle: false
  }
}
############################################# FCN body ##############################################

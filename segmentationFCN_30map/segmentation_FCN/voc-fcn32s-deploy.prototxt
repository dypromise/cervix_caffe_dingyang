  top: "fc7"
}
layer {
  name: "drop7"
  type: "Dropout"
  bottom: "fc7"
  top: "fc7"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  name: "score_fr_dy"
  type: "Convolution"
  bottom: "fc7"
  top: "score_fr"
  param {
    lr_mult: 0
    decay_mult: 0
  }
  param {
    lr_mult: 0
    decay_mult: 0
  }
  convolution_param {
    num_output: 2
    pad: 0
    kernel_size: 1
  }
}
layer {
  name: "upscore_dy"
  type: "Deconvolution"
  bottom: "score_fr"
  top: "upscore"
  param {
    lr_mult: 0
  }
  convolution_param {
    num_output: 2
    bias_term: false
    kernel_size: 64
    stride: 32
    group: 2
  }
}
############################################## prob ################################################

layer {
  name: "score_dy"
  type: "DYCrop"
  bottom: "upscore"
  bottom: "data"
  top: "score"
  dycrop_param {
    axis: 2
    offset: 8
  }
}
layer {
  name: "prob_dy"
  type: "Softmax"
  bottom: "score"
  top: "prob"
}

############################################## mask ################################################

layer {
  name: "threshold_dy"
  type: "Threshold"
  bottom: "prob"
  top: "prob_thresholded"
  threshold_param {
    threshold: 0.6
  } 
}
layer {
  name: "slice_dy"
  type: "Slice"
  bottom: "prob_thresholded"
  top: "background_dy"
  top: "mask_dy"
  slice_param {
    axis: 1
    slice_point: 1
  }
}
layer { 
  name: "mask_repeatx3" 
  type: "Convolution" 
  bottom: "mask_dy" 
  top: "mask_dy_repeatx3"
  param { lr_mult: 0 decay_mult: 0 } 
  param { lr_mult: 0 decay_mult: 0 }
  convolution_param { 
    num_output: 3  
    kernel_size: 1
    weight_filler { type: "constant" value: 1.0 }
    bias_filler { type: "constant" value: 0.0 } 
  } 
}
layer {
  name: "mask_dy/eltwise"
  type: "Eltwise"
  bottom: "data" 
  bottom: "mask_dy_repeatx3"
  top: "data_masked"
  eltwise_param {
    operation: PROD
  }
}
############################################ mask end ###############################################







